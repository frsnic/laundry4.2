<p id="notice"><%= notice %></p>

<h1>Listing Store Users</h1>

<table>
  <thead>
    <tr>
      <th>Store</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @store_users.each do |store_user| %>
      <tr>
        <td><%= store_user.store_id %></td>
        <td><%= store_user.user_id %></td>
        <td><%= link_to 'Show', store_user %></td>
        <td><%= link_to 'Edit', edit_store_user_path(store_user) %></td>
        <td><%= link_to 'Destroy', store_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store user', new_store_user_path %>
