<%= simple_form_for(@store) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :phone %>
    <%= f.input :address %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.input :annotate %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
